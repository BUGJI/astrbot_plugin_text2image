{
  "token": {
    "description": "Bot Token",
    "type": "string"
  },
  "limit": {
    "description": "对会话的限制",
    "type": "object",
    "hint": "限制性能防止爆炸",
    "items": {
      "max_images_per_task": {
        "description": "单次任务最大图片数",
        "type": "int",
